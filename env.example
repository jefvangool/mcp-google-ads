# Google Ads API Configuration
# Copy this file to .env and fill in your actual values

# OAuth Client Credentials (for OAuth authentication)
GOOGLE_ADS_CLIENT_ID=your_client_id_here.apps.googleusercontent.com
GOOGLE_ADS_CLIENT_SECRET=your_client_secret_here

# Google Ads API Configuration
GOOGLE_ADS_DEVELOPER_TOKEN=your_developer_token_here
GOOGLE_ADS_LOGIN_CUSTOMER_ID=1234567890

# Authentication Type
GOOGLE_ADS_AUTH_TYPE=oauth

# Service Account Configuration (only if using service_account auth)
GOOGLE_ADS_IMPERSONATION_EMAIL=user@yourdomain.com

# Optional: Custom credentials path
GOOGLE_ADS_CREDENTIALS_PATH=client_secret.json 